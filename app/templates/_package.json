{
    "name": "<%= themeName %>",
    "version": "0.0.0",
    "description": "<%= themeDescription %>",
    "keywords": [
	"theme", "php"
    ],
    "homepage": "<%= themeURI %>",
    "bugs" : {
	"url": "http://",
	"email": "@"
    },
    "author": {
	"name": "<%= authorName %>",
	"email": "<%= authorEmail %>",
	"url": "<%= authorURI %>"
    },
    "repository": {
	"type": "git",
	"url": "https://"
    },
    "licenses": [
	{
	    "type": "License",
	    "url": "http://"
	}
    ],
    "maintainers": [
	{
	    "name": "Bill Smith",
	    "email": "bills@example.com",
	    "web": "http://www.example.com"
	}
    ],
    "contributors": [
	{
	    "name": "",
	    "email": "@",
	    "web": "http://"
	}
    ],
    "directories": {
	"doc": "/doc"
    },

    "scripts": {

    },
    "config" : {
	"slug": "<%= _.slugify(themeName) %>",
	"sandbox": {
	    "url": "http://local.wordpress.dev ",
	    "path": "/media/Media/Workspace/test/vagrant-local/www/wordpress-default/wp-content/themes"
	}
    },
    "dependencies": {},
    "devDependencies": {
	"grunt": "~0.4.1",
	"grunt-contrib-watch": "~0.5.3",
	"grunt-open": "~0.2.0",
	"grunt-sync": "~0.0.4",
	"matchdep": "~0.3.0"
    },
    "bundledDependencies": {},
    "optionalDependencies": {},
    "engines" :
    {
	"node" : ">=0.10.3 <0.12"
    },
    "engineStrict": false,
    "os": "*",
    "cpu": "*",
    "preferGlobal": false,
    "private": true,
    "publishConfig": {}
}
